<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Responsive Eye: Peter Burr</title>
  <link rel="stylesheet" href="styles/main.css">
</head>
<body id="home">
  <div class="container">
    <section class="content">
      <!-- @import "includes/header.html" -->
      <main class="main">
        <div class="main__col left">
          <div class="intro"></div>
        </div>
        <div class="main__col right">
          <div class="square-grid">
            <div class="square-grid__square"></div>
            <div class="square-grid__square"></div>
            <div class="square-grid__square"></div>
            <div class="square-grid__square"></div>

          </div>
          <div class="square-grid__red-square"></div>
        </div>
      </main>
    </section>
    <section class="sidebar"></section>
  </div>
  <footer class="footer">
    <div class="footer__people">
      <img src="assets/images/p1.png" alt="Person 1" class="person p1">
      <img src="assets/images/p2.png" alt="Person 2" class="person p2">
      <img src="assets/images/door.png" alt="door" class="door">
    </div>
  </footer>
</body>
</html>