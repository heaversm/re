{
  "name": "re",
  "version": "1.0.0",
  "description": "Responsive Eye gallery show site.",
  "scripts": {
    "start": "parcel src/index.html --port 3000 --no-hmr",
    "build": "parcel build src/index.html",
    "build:hooliganship": "npm run build -- --public-url /emptyspace/"
  },
  "author": "",
  "license": "UNLICENSED",
  "repository": {},
  "browserslist": ">0.2%, last 2 versions, Firefox ESR, not dead",
  "dependencies": {
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/inspector": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "core-js": "^3.8.1",
    "micromodal": "^0.4.6",
    "oimo": "^1.0.9",
    "p5": "^1.2.0",
    "recast-detour": "^1.1.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.16.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-structurize": "^2.3.2",
    "rimraf": "^2.7.1",
    "sass": "^1.30.0"
  },
  "parcel-plugin-structurize": {
    "rules": [
      {
        "match": "*.{js,js.map}",
        "folder": "scripts"
      },
      {
        "match": "*.{css,css.map}",
        "folder": "styles"
      },
      {
        "match": "*.{jpg,png,gif,svg}",
        "folder": "assets/images"
      },
      {
        "match": "*.{ttf,otf}",
        "folder": "assets/fonts"
      },
      {
        "match": "*.babylon",
        "folder": "assets/models"
      }
    ]
  }
}
