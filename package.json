{
  "name": "re",
  "version": "1.0.0",
  "description": "Responsive Eye gallery show site.",
  "scripts": {
    "start": "parcel src/index.html --port 3000 --no-hmr",
    "build": "parcel build src/index.html",
    "prebuild": "rimraf dist"
  },
  "author": "",
  "license": "UNLICENSED",
  "repository": {},
  "browserslist": ">0.2%, last 2 versions, Firefox ESR, not dead",
  "dependencies": {
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/inspector": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "core-js": "^3.8.1",
    "micromodal": "^0.4.6",
    "oimo": "^1.0.9",
    "p5": "^1.2.0",
    "recast-detour": "^1.1.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.16.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "rimraf": "^2.7.1",
    "sass": "^1.30.0"
  },
  "customDistStructure": {
    "config": {
      "scripts": [
        ".js",
        ".js.map"
      ],
      "styles": [
        ".css",
        ".css.map"
      ],
      "assets/images": [
        ".jpg",
        ".png",
        ".gif",
        ".svg"
      ],
      "assets/fonts": [
        ".ttf",
        ".otf"
      ]
    },
    "options": {
      "development": true
    }
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/assets/models",
        "staticOutDir": "assets/models"
      }
    ],
    "watcherGlob": "**"
  }
}
